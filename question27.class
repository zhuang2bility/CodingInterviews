public class Solution {
    public int FindGreatestSumOfSubArray(int[] array) {
        int sum=0;
        int result=0x80000000;
        int len=array.length;
        if(array==null||len<=0) return result;
        for(int i=0;i<len;++i){
            if(sum<=0){
                sum=array[i];
            }else{
            sum+=array[i];
            }
            if(sum>result) result=sum;
        }
        return result;
    }
}
