public class Solution {
    public int Fibonacci(int n) {
        if(n<=0) return 0;
        if(n==1) return 1;
        int fibN=0,fibNMinusOne=1,fibNMinusTwo=0;
        for(int i=2;i<=n;i++){
            fibN=fibNMinusOne+fibNMinusTwo;
            fibNMinusTwo=fibNMinusOne;
            fibNMinusOne=fibN;
        }
        return fibN;
    }
}
